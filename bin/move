#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to move? Select 1-9, 1 is top left, 9 is bottom right."
    selection = gets.strip
move(board, selection)
display_board(board)
